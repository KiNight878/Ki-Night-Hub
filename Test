-- LocalScript dentro de StarterPlayerScripts

-- Orion Library (inclua o c√≥digo completo da biblioteca aqui)
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

-- Criar a janela do menu
local Window = OrionLib:MakeWindow({
    Name = "Ki Night Save",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "OrionTest"
})

-- Fun√ß√£o para criar bot√µes com os scripts correspondentes
local function criarBotao(nome, script, aba)
    aba:AddButton({
        Name = nome,
        Callback = function()
            print("Bot√£o " .. nome .. " pressionado")
            loadstring(script)()
        end
    })
end

-- Criar as abas solicitadas
local AnimeFightersTab = Window:MakeTab({
