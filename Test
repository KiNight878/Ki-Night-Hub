-- Carregando a biblioteca Orion
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

-- Criando uma janela com o nome "Kai Night Save Hub"
local Window = OrionLib:MakeWindow({Name = "Kai Night Save Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "KaiNightSaveHubConfig"})

-- Criando uma aba "Blox Fruits"
local Tab = Window:MakeTab({
    Name = "Blox Fruits",
    PremiumOnly = false
})

-- Adicionando um botão "W-azure"
Tab:AddButton({
    Name = "W-azure",
    Callback = function()
        print("Botão W-azure clicado")
        getgenv().Team = "Pirates"
        getgenv().AutoLoad = false
        getgenv().SlowLoadUi = false
        getgenv().ForceUseSilentAimDashModifier = false
        getgenv().ForceUseWalkSpeedModifier = false
        loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/3b2169cf53bc6104dabe8e19562e5cc2.lua"))()
    end    
})

-- Iniciando a GUI
OrionLib:Init()
